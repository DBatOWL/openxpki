{{!-- 
  The Content below gets added to the documents <head> by ember-cli-head.
  The 'model' available here is an instance of the 'head-data' service
  as defined in app/services/head-data.js.
--}}
<title>{{this.model.config.pageTitle}}</title>
<meta property="og:title" content={{this.model.config.pageTitle}} />

{{#if this.model.config.customCSSUrl}}
  <link integrity="" rel="stylesheet" href="{{this.model.config.customCSSUrl}}">
{{/if}}

{{#if this.model.config.customCSS}}
  <style type="text/css">
    {{defuse this.model.config.customCSS}}
  </style>
{{/if}}
